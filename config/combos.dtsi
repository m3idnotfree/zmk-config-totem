/* Vertical combos - left hand */
ZMK_COMBO(at,    &kp AT,        LT3 LM3,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(hash,  &kp HASH,      LT2 LM2,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(dllr,  &kp DLLR,      LT1 LM1,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(prcnt, &kp PRCNT,     LT0 LM0,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

ZMK_COMBO(grave, &kp GRAVE,     LM3 LB3,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(bslh,  &kp BSLH,      LM2 LB2,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(equal, &kp EQUAL,     LM1 LB1,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(tilde, &kp TILDE,     LM0 LB0,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

/* Vertical combos - right hand */
ZMK_COMBO(caret, &kp CARET,     RT0 RM0,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(plus,  &kp PLUS,      RT1 RM1,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(star,  &kp STAR,      RT2 RM2,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(amps,  &kp AMPS,      RT3 RM3,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

ZMK_COMBO(under, &kp UNDER,     RM0 RB0,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(minus, &kp MINUS,     RM1 RB1,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(fslh,  &kp FSLH,      RM2 RB2,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(pipe,  &kp PIPE,      RM3 RB3,     DEF QWE NAV NUM, COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

/* Thumb combos left - hand */
ZMK_COMBO(v_up,  &kp C_VOL_UP,  LT0 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(v_dn,  &kp C_VOL_DN,  LM0 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(t_l,   &kp LEFT,      LM4 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(t_r,   &kp RIGHT,     LM2 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(t_u,   &kp UP,        LT3 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(t_d,   &kp DOWN,      LM3 LH1,     DEF QWE         , COMBO_TERM_FAST, COMBO_IDLE_FAST)

/* Thumb combos right hand */
// ZMK_COMBO(rpipe,  &kp PIPE,      RT3 RH1,      DEF QWE        , COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
// ZMK_COMBO(rbslh,  &kp BSLH,      RT2 RH1,      DEF QWE        , COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
// ZMK_COMBO(rtilde, &kp TILDE,     RT1 RH1,      DEF QWE        , COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

/* Mac Web combes */
ZMK_COMBO(close_tap,   &kp LG(W),        RT2 RT3    , MOUSE, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(new_tap,     &kp LG(T),        RT1 RT2 RT3, MOUSE, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(refresh_tap, &kp F5,           RT1 RT3    , MOUSE, COMBO_TERM_FAST, COMBO_IDLE_FAST)

     
