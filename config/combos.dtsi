/* Copied (modified) from https://github.com/caksoylar/zmk-config */
#define COMBO(NAME, BINDINGS, KEYPOS, LAYERS, TERM, QUICKTAP)                                      \
  combo_##NAME {                                                                                   \
    timeout-ms = <TERM>;                                                                           \
    bindings = <BINDINGS>;                                                                         \
    key-positions = <KEYPOS>;                                                                      \
    layers = <LAYERS>; \
    require-prior-idle-ms = <QUICKTAP>; \
  };

#define COMBO_TERM_FAST 18
#define COMBO_TERM_SLOW 30

#define COMBO_IDLE_FAST 150
#define COMBO_IDLE_SLOW 50

// make homerow combos tap-only (so HRMs can be chorded instantaneously)
#define HRC_TAB    &hml         LS(LALT) TAB
#define HRC_REPEAT &hml_repeat  LS(LCTRL) 0
#define HRC_LPAR   &hmr_lpar_lt RS(LCTRL) 0
#define HRC_RPAR   &hmr_rpar_gt RS(LALT) 0

/ {
    combos {
        compatible = "zmk,combos";

/* Hangle */
ZMK_COMBO(defqwe,&tog QWE,    RB3 RB2 RB1,   DEF QWE        ,  COMBO_TERM_SLOW, COMBO_IDLE_SLOW)
ZMK_COMBO(hangle,&tog_hangle, LB3 LB2 LB1,   DEF QWE        , COMBO_TERM_SLOW, COMBO_IDLE_SLOW)

/* Horizontal combos - left hand */
ZMK_COMBO(esc,   &kp ESC,       LT3 LT2,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(ret,   &kp RETURN,    LT2 LT1,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(mouse, &smart_mouse,  LT3 LT1,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)

ZMK_COMBO(tab,   HRC_TAB,       LM3 LM2,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(repeat,HRC_REPEAT,    LM2 LM1,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)

ZMK_COMBO(cut,   &kp LC(X),     LB3 LB1,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(copy,  &kp LC(INS),   LB3 LB2,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(paste, &kp LS(INS),   LB2 LB1,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)

/* Horizontal combos - right hand */
ZMK_COMBO(bspc,  &kp BSPC,      RT1 RT2,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(del,   &kp DEL,       RT2 RT3,     DEF QWE NAV NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)

ZMK_COMBO(lpar,  HRC_LPAR,      RM1 RM2,     DEF QWE     NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(rpar,  HRC_RPAR,      RM2 RM3,     DEF QWE     NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(lt,    &kp LT,        RM1 RM2,         NAV        , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(gt,    &kp GT,        RM2 RM3,         NAV        , COMBO_TERM_FAST, COMBO_IDLE_FAST)

ZMK_COMBO(lbkt,  &kp LBKT,      RB1 RB2,     DEF QWE    NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(rbkt,  &kp RBKT,      RB2 RB3,     DEF QWE    NUM, COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(lbrc,  &kp LBRC,      RB1 RB2,         NAV       , COMBO_TERM_FAST, COMBO_IDLE_FAST)
ZMK_COMBO(rbrc,  &kp RBRC,      RB2 RB3,         NAV       , COMBO_TERM_FAST, COMBO_IDLE_FAST)

